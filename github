// First time from new folder
git init
git add .
git commit -m "first commit"
git remote add origin https://github.com/Aakashdeveloper/learngit.git
git push -u origin master

// Next time from same folder
git add .
git commit -m "first commit"
git push


///new branch
git checkout -b branchname


////
go to main branch > take pull > create new branch
git checkout Master
git pull
git checkout -b branchname
